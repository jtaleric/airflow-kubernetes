{
    "benchmarks": [
        {
            "name": "scale",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "150",
                "METADATA_COLLECTION": "true",
                "WORKLOAD_NODE_ROLE": "workload"
            }
        },
        {
            "name": "cluster_density",
            "workload": "kube-burner",
            "command": "./run_clusterdensity_test_fromgit.sh",
            "env": {
                "JOB_ITERATIONS":"2000",
                "JOB_TIMEOUT":"18000",
                "STEP_SIZE": "30s",
                "QPS":"20",
                "BURST":"20",
                "METRICS_PROFILE":"metrics-aggregated.yaml",
                "LOG_LEVEL":"info",
                "LOG_STREAMING":"true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true"
            }
        },
        {
            "name": "node_density",
            "workload": "kube-burner",
            "command": "./run_nodedensity_test_fromgit.sh",
            "env": {
                "PODS_PER_NODE":"250",
                "NODE_COUNT":"25",
                "JOB_TIMEOUT": "18000",
                "QPS":"20",
                "BURST":"20",
                "STEP_SIZE": "30s",
                "METRICS_PROFILE":"metrics.yaml",
                "LOG_LEVEL":"info",
                "LOG_STREAMING":"true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true"
            }
        }
    ]
}